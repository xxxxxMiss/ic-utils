{
  "name": "ic-tools",
  "version": "1.0.0",
  "description": "a toolkit for ic-project",
  "main": "commitlint.config.js",
  "scripts": {
    "pub": "sudo lerna publish --conventional-commits",
    "commitmsg": "commitlint -e $GIT_PARMAS",
    "test": "ava",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development node webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production node webpack.config.js",
    "create": "node scripts/create.js"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.49",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "ava": "1.0.0-beta.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "lerna": "^2.11.0",
    "lodash": "^4.17.10",
    "nyc": "^11.8.0",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.0.0",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxxxxMiss"
  },
  "keywords": [
    "toolkit",
    "ic"
  ],
  "packagePrefix": "ic-",
  "ava": {
    "files": [
      "packages/**/test/*.js"
    ],
    "require": [
      "./helpers/setup.js",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx",
          "@babel/plugin-syntax-dynamic-import"
        ],
        "presets": [
          "@babel/preset-env",
          "@ava/stage-4"
        ],
        "babelrc": false
      }
    }
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "author": "xxxxxMiss <xxxxxmiss@gmail.com> (https://github.com/xxxxxMiss)",
  "license": "ISC"
}
