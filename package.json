{
  "name": "ic-tools",
  "version": "1.0.0",
  "description": "a toolkit for ic-project",
  "main": "commitlint.config.js",
  "scripts": {
    "pub": "npm run build:prod && sudo lerna publish --conventional-commits",
    "commitmsg": "commitlint -e $GIT_PARMAS",
    "test": "jest",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development node webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production node webpack.config.js",
    "create": "node scripts/create.js",
    "docs": "node scripts/docs.js",
    "docs:dev": "npm run docs && vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "dayjs": "^1.6.10",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-serializer-vue": "^2.0.2",
    "lerna": "^2.11.0",
    "lodash": "^4.17.10",
    "nyc": "^11.9.0",
    "signale": "^1.2.1",
    "sinon": "^6.0.1",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.16",
    "vue-jest": "^2.6.0",
    "vuepress": "^0.10.2",
    "webpack": "^4.13.0",
    "webpack-cli": "^2.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxxxxMiss"
  },
  "keywords": [
    "toolkit",
    "ic"
  ],
  "packagePrefix": "ic-",
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/packages/$1/src"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "ava": {
    "files": [
      "packages/**/test/*.spec.js"
    ],
    "require": [
      "./helpers/setup.js",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx",
          "@babel/plugin-syntax-dynamic-import"
        ],
        "presets": [
          "@babel/preset-env"
        ],
        "babelrc": false
      },
      "extensions": [
        "js",
        "jsx"
      ]
    }
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "author": "xxxxxMiss <xxxxxmiss@gmail.com> (https://github.com/xxxxxMiss)",
  "license": "ISC",
  "dependencies": {
    "joi-browser": "^13.4.0",
    "serialize-javascript": "^1.5.0"
  }
}
