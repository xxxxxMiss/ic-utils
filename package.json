{
  "name": "ic-tools",
  "version": "1.0.0",
  "description": "a toolkit for ic-project",
  "main": "commitlint.config.js",
  "scripts": {
    "pub": "npm run build:prod && sudo lerna publish --conventional-commits",
    "commitmsg": "commitlint -e $GIT_PARMAS",
    "test": "ava",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development node webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production node webpack.config.js",
    "create": "node scripts/create.js",
    "docs": "node scripts/docs.js",
    "docs:dev": "npm run docs && vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.49",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "ava": "1.0.0-beta.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.3",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.5",
    "dayjs": "^1.6.9",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "lerna": "^2.11.0",
    "lodash": "^4.17.10",
    "nyc": "^11.8.0",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.0.0",
    "signale": "^1.2.1",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.16",
    "vuepress": "^0.10.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxxxxMiss"
  },
  "keywords": [
    "toolkit",
    "ic"
  ],
  "packagePrefix": "ic-",
  "ava": {
    "files": [
      "packages/**/test/*.js"
    ],
    "require": [
      "./helpers/setup.js",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx",
          "@babel/plugin-syntax-dynamic-import"
        ],
        "presets": [
          "@babel/preset-env"
        ],
        "babelrc": false
      }
    }
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "author": "xxxxxMiss <xxxxxmiss@gmail.com> (https://github.com/xxxxxMiss)",
  "license": "ISC"
}
